{
  "name": "oxid-professional-services/oxid-modules-config",
  "description": "OXID core and module configurations importer and exporter",
  "type": "oxideshop-module",
  "keywords": ["oxid", "modules", "eShop", "oxconfig", "config", "export"],
  "license": "GPL-3.0-only",
  "require": {
    "symfony/yaml": "^2.0 || ^3.0",
    "oxid-esales/oxideshop-ce": "^v6.0.0",
    "oxid-community/moduleinternals": "> 1.5"
  },
  "require-dev": {
    "oxid-community/moduleinternals": "^3.0@alpha"
  },
  "extra": {
    "oxideshop": {
      "target-directory": "oxps/modulesconfig",
      "blacklist-filter": [
                "Core/**/*",
                "Command/**/*",
                "Controller/**/*",
                "Model/**/*",
                ".*"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "OxidProfessionalServices\\ModulesConfig\\": "../../../source/modules/oxps/modulesconfig"
    }
  }
}
